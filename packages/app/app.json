{
  "expo": {
    "name": "petfabula-app",
    "slug": "petfabula-app",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/app_icon.png",
    "scheme": "myappredirect",
    "splash": {
      "image": "./assets/app_splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#fe716b"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": ["**/*"],
    "userInterfaceStyle": "automatic",
    "ios": {
      "buildNumber": "1.0.2",
      "supportsTablet": false,
      "usesAppleSignIn": true,
      "bundleIdentifier": "com.petfabula.petfabula",
      "infoPlist": {
        "CFBundleDevelopmentRegion": "ja_JP",
        "NSCameraUsageDescription": "カメラにアクセスして写真をアップロードします",
        "NSPhotoLibraryUsageDescription": "写真ライブラリーをアクセスして写真をアップロードします"
      }
    },
    "android": {
      "permissions": ["CAMERA"],
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#FFFFFF"
      },
      "package": "com.petfabula.petfabula",
      "versionCode": 1
    },
    "web": {
      "favicon": "./assets/favicon.png"
    }
  }
}
